
LIBGNAT_SRCS += system.ads

configure-rts-target :
ifeq ($(OSTYPE),cmd)
	$(PROCESSCFG) $(RTS_SOURCE_PATH_TARGET_CMD)\system.ads.in $(ADAINCLUDE_PATH_CMD)\system.ads
else
	$(PROCESSCFG) $(RTS_SOURCE_PATH_TARGET)/system.ads.in $(ADAINCLUDE_PATH)/system.ads
endif
ifneq (,$(filter $(OSTYPE),cmd msys))
	$(DOS2UNIX) $(ADAINCLUDE_PATH_CMD)\system.ads $(ADAINCLUDE_PATH_CMD)\system.ads
endif
ifeq ($(OSTYPE),cmd)
	$(CP) $(RTS_SOURCE_PATH_TARGET_CMD)\rts.cs.in $(RTS)\$(TOOLCHAIN_NAME)\ $(NULL)
else
	$(CP) $(RTS_SOURCE_PATH_TARGET)/rts.cs.in ./$(RTS)/$(TOOLCHAIN_NAME)/
endif

