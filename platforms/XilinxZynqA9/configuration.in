
#
# XilinxÂ® Zynq A9 board (QEMU emulator)
#

CPU       := ARM
CPU_MODEL := CORTEXA9

GCC_SWITCHES_PLATFORM     += -mcpu=cortex-a9
STARTUP_OBJECTS           := startup.S llkernel.S
LD_SWITCHES_PLATFORM      +=
OBJDUMP_SWITCHES_PLATFORM +=
POSTBUILD_ROMFILE         := Y
OBJCOPY_SWITCHES_PLATFORM += -O binary --gap-fill=0x00

USE_APPLICATION := test-XilinxZynqA9

SESSION_START_COMMAND :=
SESSION_END_COMMAND   :=

RUN_COMMAND   := $(RUNSWEETADA) -k "$(SWEETADA_PATH)" -p "$(PLATFORM_DIRECTORY)" -f qemu.cfg
DEBUG_COMMAND := $(RUN_COMMAND) -d DEBUG_QEMU

