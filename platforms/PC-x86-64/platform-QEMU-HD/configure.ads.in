
package Configure
   with Pure => True
   is

   -- PLATFORM and CPU definitions
   PLATFORM    : constant String := "PC-x86-64";
   SUBPLATFORM : constant String := "QEMU-HD";
   CPU         : constant String := "@CPU@";
   CPU_MODEL   : constant String := "@CPU_MODEL@";

   -- basic configuration parameters
   PC             : constant Boolean := True;
   TICK_FREQUENCY : constant := 1_000;

   -- IOEMU
   USE_QEMU_IOEMU : constant Boolean := @USE_QEMU_IOEMU@;

end Configure;
