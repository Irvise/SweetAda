
package Configure
   with Pure => True
   is

   -- PLATFORM and CPU definitions
   PLATFORM  : constant String := "QEMU-PPC64";
   CPU       : constant String := "@CPU@";
   CPU_MODEL : constant String := "@CPU_MODEL@";

   -- IOEMU
   USE_QEMU_IOEMU : constant Boolean := @USE_QEMU_IOEMU@;

end Configure;
