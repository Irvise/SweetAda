
#
# STM32F769I
#
# ARM Cortex-M7 LE
#

CPU       := ARM
CPU_MODEL := CORTEXM7

GCC_SWITCHES_PLATFORM     += -mcpu=cortex-m7 -Wa,-mcpu=cortex-m7
LD_SWITCHES_PLATFORM      +=
OBJDUMP_SWITCHES_PLATFORM +=
POSTBUILD_ROMFILE         := Y
OBJCOPY_SWITCHES_PLATFORM += --output-target=binary --gap-fill=0x00

USE_APPLICATION := dummy

SESSION_START_COMMAND :=
SESSION_END_COMMAND   :=

#RUN_COMMAND   := "$(SWEETADA_PATH)"/$(PLATFORM_DIRECTORY)/startopenocd.sh
RUN_COMMAND   := "$(TCLSH)" "$(SWEETADA_PATH)"/$(PLATFORM_DIRECTORY)/programmer.tcl
DEBUG_COMMAND :=

