
package Configure
   with Pure => True
   is

   -- PLATFORM and CPU definitions
   PLATFORM : constant String := "SPARCstation5";
   CPU      : constant String := "@CPU@";

   -- basic configuration parameters
   BOOT_FROM_NETWORK : constant Boolean := @BOOT_FROM_NETWORK@;
   USE_QEMU          : constant Boolean := @USE_QEMU@;

   -- IOEMU
   USE_QEMU_IOEMU : constant Boolean := @USE_QEMU_IOEMU@;

end Configure;
