
package Configure is

   pragma Pure;

   -- PLATFORM and CPU definitions
   PLATFORM  : constant String := "Dreamcast";
   CPU       : constant String := "@CPU@";
   CPU_MODEL : constant String := "@CPU_MODEL@";

   -- basic configuration parameters
   BOOT_TYPE : constant String := "@BOOT_TYPE@";
   GXEMUL    : constant String := "@GXEMUL@";

   -- IOEMU
   USE_GXEMUL_IOEMU : constant Boolean := @USE_GXEMUL_IOEMU@;

end Configure;
