
#
# STM32VLDISCOVERY (QEMU emulator)
#
# STM32F100xx/STM32F405 ARM Cortex-M3 LE (ARMv7-M)
#

# force ZFP
RTS := zfp
PROFILE := zfp
USE_LIBADA := N

CPU       := ARM
CPU_MODEL := CORTEXM3

GCC_SWITCHES_PLATFORM     := -mcpu=cortex-m3
STARTUP_OBJECTS           := startup.S llkernel.S
LD_SWITCHES_PLATFORM      := --undefined=vectors
OBJDUMP_SWITCHES_PLATFORM :=
POSTBUILD_ROMFILE         := Y
OBJCOPY_SWITCHES_PLATFORM := -O binary --gap-fill=0x00
CONFIGURE_FILES_PLATFORM  := configure.ads configure.h

export USE_QEMU_IOEMU := False

USE_APPLICATION := test-STM32VLDISCOVERY

SESSION_START_COMMAND :=
SESSION_END_COMMAND   :=

RUN_COMMAND   := "$(SWEETADA_PATH)"/$(PLATFORM_DIRECTORY)/qemu$(SCREXT)
DEBUG_COMMAND := "$(SWEETADA_PATH)"/$(PLATFORM_DIRECTORY)/qemu$(SCREXT) -debug

