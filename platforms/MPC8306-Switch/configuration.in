
#
# MPC8306 "COMESA Switch" CPU board
#

CPU := PowerPC

# -meabi -msdata=eabi
GCC_SWITCHES_PLATFORM     += -mcpu=e300c3 -meabi -msoft-float -Wa,-me300 -Wa,-mregnames
LD_SWITCHES_PLATFORM      +=
OBJDUMP_SWITCHES_PLATFORM +=
POSTBUILD_ROMFILE         := Y
OBJCOPY_SWITCHES_PLATFORM += --output-target=binary --gap-fill=0x00

USE_APPLICATION := test-MPC8306-Switch

SESSION_START_COMMAND := "$(SWEETADA_PATH)"/$(PLATFORM_DIRECTORY)/ccs-exec.sh
SESSION_END_COMMAND   :=

RUN_COMMAND   :=
DEBUG_COMMAND :=

