
#
# Amiga (FS-UAE emulator)
#
# M68030
#

CPU       := M68k
CPU_MODEL := M68030
FPU_MODEL := M68881

GCC_SWITCHES_PLATFORM     := -mcpu=68030 -m68881
STARTUP_OBJECTS           := startup.S llkernel.S
LD_SWITCHES_PLATFORM      :=
OBJDUMP_SWITCHES_PLATFORM :=
POSTBUILD_ROMFILE         := Y
OBJCOPY_SWITCHES_PLATFORM := -O binary --gap-fill=0x00

LD_SCRIPT := linker.lds-rom
#LD_SCRIPT := linker.lds-chipram # S-record download
#LD_SCRIPT := linker.lds-fastram # S-record download

export USE_FS_UAE_IOEMU := False

USE_APPLICATION := test-Amiga-FS-UAE

SESSION_START_COMMAND :=
SESSION_END_COMMAND   :=

RUN_COMMAND   := $(SHELL_EXEC) $(PLATFORM_DIRECTORY)/fs-uae$(SCREXT)
DEBUG_COMMAND := $(SHELL_EXEC) $(PLATFORM_DIRECTORY)/fs-uae$(SCREXT) -debug

