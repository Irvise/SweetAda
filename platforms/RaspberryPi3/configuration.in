
#
# Raspberry Pi 3
#
# BCM2837 4-core Cortex-A53 AArch64 LE
#

CPU       := AArch64
CPU_MODEL := Cortex-A53

GCC_SWITCHES_PLATFORM     += -mcpu=cortex-a53 -mabi=lp64 -mlittle-endian -Wa,-EL
LD_SWITCHES_PLATFORM      +=
OBJDUMP_SWITCHES_PLATFORM +=
POSTBUILD_ROMFILE         := Y
OBJCOPY_SWITCHES_PLATFORM += --output-target=binary --gap-fill=0x00

USE_APPLICATION := dummy

# path to FAT32 1st partition on a micro-SD memory card
MSDCARD_MOUNTPOINT := /mnt/hd
export MSDCARD_MOUNTPOINT

SESSION_START_COMMAND :=
SESSION_END_COMMAND   :=

RUN_COMMAND   :=
DEBUG_COMMAND :=

